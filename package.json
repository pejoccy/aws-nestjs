{
   "name": "orysx-api",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "private": true,
   "license": "UNLICENSED",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "postbuild": "yarn copy-files",
      "copy-files": "copyfiles src/common/mailer/templates/*.ejs dist",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config typeorm-cli.config.ts",
      "migration:generate": "npm run typeorm migration:generate -- -n",
      "migration:create": "npm run typeorm migration:create -- -n",
      "migration:run": "npm run typeorm migration:run",
      "migration:revert": "npm run typeorm migration:revert"
   },
   "dependencies": {
      "@nestjs-modules/mailer": "^1.6.1",
      "@nestjs/bull": "^0.5.5",
      "@nestjs/common": "^8.0.0",
      "@nestjs/config": "^2.1.0",
      "@nestjs/core": "^8.0.0",
      "@nestjs/jwt": "^8.0.1",
      "@nestjs/passport": "^8.2.1",
      "@nestjs/platform-express": "^8.0.0",
      "@nestjs/swagger": "^5.2.1",
      "@nestjs/typeorm": "^8.0.2",
      "@sendgrid/mail": "^7.7.0",
      "@sentry/node": "^7.1.1",
      "@types/cache-manager": "^4.0.0",
      "@types/cache-manager-redis-store": "^2.0.1",
      "aws-sdk": "^2.1157.0",
      "bcrypt": "^5.0.1",
      "bull": "^4.8.3",
      "cache-manager": "^4.0.1",
      "cache-manager-redis-store": "^2.0.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.13.2",
      "copyfiles": "^2.4.1",
      "crypto-js": "^4.1.1",
      "dotenv": "^16.0.1",
      "mime-types": "^2.1.35",
      "moment": "^2.29.3",
      "multer": "^1.4.5-lts.1",
      "nanoid": "^3.3.4",
      "nestjs-real-ip": "^2.1.0",
      "nestjs-typeorm-paginate": "^4.0.1",
      "nodemailer": "^6.7.5",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.0",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^3.0.2",
      "rxjs": "^7.2.0",
      "smpp": "^0.6.0-rc.4",
      "swagger-ui-express": "^4.4.0",
      "typeorm": "0.2.37"
   },
   "devDependencies": {
      "@nestjs/cli": "^8.0.0",
      "@nestjs/schematics": "^8.0.0",
      "@nestjs/testing": "^8.0.0",
      "@types/bcrypt": "^5.0.0",
      "@types/bull": "^3.15.8",
      "@types/crypto-js": "^4.1.1",
      "@types/express": "^4.17.13",
      "@types/jest": "27.5.0",
      "@types/lodash": "^4.14.182",
      "@types/mime-types": "^2.1.1",
      "@types/multer": "^1.4.7",
      "@types/node": "^16.11.10",
      "@types/nodemailer": "^6.4.4",
      "@types/passport-jwt": "^3.0.6",
      "@types/supertest": "^2.0.11",
      "@typescript-eslint/eslint-plugin": "^5.0.0",
      "@typescript-eslint/parser": "^5.0.0",
      "eslint": "^8.0.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "28.0.3",
      "prettier": "^2.3.2",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "28.0.1",
      "ts-loader": "^9.2.3",
      "ts-node": "10.7.0",
      "tsconfig-paths": "4.0.0",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
